{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "light-dashboard-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "sms",
      "assets": [
        "assets",
        "favicon.ico"

        
        
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "./assets/css/bootstrap.min.css",
        "./assets/css/light-bootstrap-dashboard.css",
        "../node_modules/datatables.net-dt/css/jquery.dataTables.css",
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "./assets/css/select2.min.css",
        "./assets/css/pnotify.custom.min.css"
      ],
      "scripts": [
        "./assets/js/core/jquery.3.2.1.min.js",
        "./assets/js/core/popper.min.js",
        "./assets/js/core/bootstrap.min.js",
        "./assets/js/light-bootstrap-dashboard.js-v=2.0.1.htm",
        "./assets/js/jquery.dataTables.js",
        "./assets/js/select2.min.js",
        "./assets/js/pnotify.custom.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "class": {
      "spec": false
    },
    "component": {}
  }
}
